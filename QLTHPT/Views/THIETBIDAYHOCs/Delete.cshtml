@model QLTHPT.ModelDB.THIETBIDAYHOC

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3><strong>Xóa Thông Tin Thiết Bị Dạy Học</strong></h3>

<h5>Bạn có chắc muốn xóa ?</h5>
<div>
  
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã Thiết Bị Dạy Học
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TBDH_MA)
        </dd>

        <dt>
            Tên Thiết Bị Dạy Học
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TBDH_TEN)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" /> |
            @Html.ActionLink("Quay Lại", "Index")
        </div>
    }
</div>

@model QLTHPT.ModelDB.THOIKHOABIEU

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h3><strong>Xóa Thông Tin Thời Khóa Biểu</strong></h3>

    <h5>Bạn có chắc muốn xóa ?</h5>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã Thời Khóa Biểu
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TKB_MA)
        </dd>
        <dt>
            Cán Bộ
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CANBO.CB_HOTEN)
        </dd>

        <dt>
            Môn Học
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MONHOC.MH_TEN)
        </dd>

        <dt>
            Tiết Học
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TIETHOC.TH_TEN)
        </dd>

        <dt>
            Thứ
        </dt>

        <dd>
            @Html.DisplayFor(model => model.THU.THU_TEN)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" /> |
            @Html.ActionLink("Quay Lại", "Index")
        </div>
    }
</div>

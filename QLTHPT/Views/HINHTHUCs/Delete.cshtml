@model QLTHPT.ModelDB.HINHTHUC

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3><strong>XÓA</strong></h3>

<h5>Bạn có chắc muốn xóa ?</h5>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã Hình Thức
        </dt>

        <dd>
            @Html.DisplayFor(model => model.HT_MA)
        </dd>


        <dt>
            Tên Hình Thức
        </dt>

        <dd>
            @Html.DisplayFor(model => model.HT_TEN)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" /> |
            @Html.ActionLink("Quay lại", "Index")
        </div>
    }
</div>

@model QLTHPT.ModelDB.UpdateImage

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3><strong>Xóa Hình Ảnh</strong></h3>

<h5>Bạn có chắc muốn xóa ?</h5>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã Hình Ảnh
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt>
            Image Url
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Image_url)
        </dd>

        <dt>
            Tên Hình Ảnh
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name_im)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" /> |
            @Html.ActionLink("Quay Lại", "Index")
        </div>
    }
</div>

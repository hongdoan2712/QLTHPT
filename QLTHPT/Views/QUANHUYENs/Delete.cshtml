@model QLTHPT.ModelDB.QUANHUYEN

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3><strong>XÓA</strong></h3>

<h3>Bạn có chắc muốn xóa ?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã Quận Huyện
        </dt>

        <dd>
            @Html.DisplayFor(model => model.QH_MA)
        </dd>

        <dt>
            Tên Quận Huyện
        </dt>

        <dd>
            @Html.DisplayFor(model => model.QH_TEN)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" /> |
            @Html.ActionLink("Quay lại", "Index")
        </div>
    }
</div>

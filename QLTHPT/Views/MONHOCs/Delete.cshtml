@model QLTHPT.ModelDB.MONHOC

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h3><strong>Xóa Thông Tin Môn Học</strong></h3>

    <h5>Bạn có chắc muốn xóa ?</h5>
<div>

    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã Môn Học
        <dd>
            @Html.DisplayFor(model => model.MH_MA)
        </dd>
        <dt>
            Tên Môn Học
        <dd>
            @Html.DisplayFor(model => model.MH_TEN)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

<div class="form-actions no-color">
    <input type="submit" value="Xóa" class="btn btn-default" /> |
    @Html.ActionLink("Quay Lại", "Index")
</div>
    }
</div>

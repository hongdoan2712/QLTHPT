@model QLTHPT.ModelDB.CHUYENNGANH

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3><strong>Xóa</strong></h3>

<h5>Bạn có chắc muốn xóa ?</h5>
<div>
    <hr />
    <dl class="dl-horizontal">

        <dt>
            Mã Chuyên Ngành
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CN_MA)
        </dd>


        <dt>
            Tên Chuyên Ngành
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CN_TEN)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" /> |
            @Html.ActionLink("Quay lại", "Index")
        </div>
    }
</div>
